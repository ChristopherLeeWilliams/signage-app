<!DOCTYPE html>
<html>
<head>
<script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>

<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
<style>
  
</style>

</head>
<body style="margin:2%; text-align:center">
  
  <table class="table table-striped table-dark">
    <thead>
      <tr>
        <th scope="col"><h1>Schedule ID</h1></th>
      </tr>
    </thead>
    <tbody id="fileLinks">
          
    </tbody>
  </table>
  
  <script>
    $( document ).ready(function() {
      const roomName = "{{roomName}}";
      let files = "{{dates}}";
      files = files.split(",");
      
      let htmlString = "";
      
      files.forEach(function(file) {
        htmlString += '<tr><th scope="row"><a href="/admin/room/'+roomName+'/'+file+'">'+file+'</a></th></tr>';
      });
      
      $('#fileLinks').append($(htmlString));
      
    });
  </script>
  <a type="button" class="btn btn-primary" href="/createDate/{{roomName}}"> Create Date </a>
</body>
</html>